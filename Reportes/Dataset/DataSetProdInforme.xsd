                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT         Productos_1.IdEmbalaje, Embalaje.EmbalajeE, Productos_1.IdProducto, Productos_1.Descripcion, GrupoProductos.IdProductoGrupo, GrupoProductos.NombreProductoGrupo, Productos_1.KiloUnidad, 
                         Productos_1.UnidadEmbalaje, Productos_1.Desviacion, Tercero.RazonSocial, Productos_1.IdCliente, Productos_1.IdEmpresa, Productos_1.IdGrupo, Productos_1.IdEstado, Productos_1.PromHistorico, ProductosCodigos.Codigo,
                          ProductosCodigos.IdProducto AS Expr1, Productos_1.SeEmpaca, Productos_1.IdEmbalajeEmpaque, Productos_1.IdProductoEmpacar, ISNULL
                             ((SELECT        Descripcion
                                 FROM            Productos
                                 WHERE        (IdProducto = Productos_1.IdProductoEmpacar)), '') AS nombreProducto, Productos_1.UnidadEmbalajeEmpacar, Productos_1.CantidadEstibaEmpacar
FROM            Productos AS Productos_1 INNER JOIN
                         Embalaje ON Productos_1.IdEmbalaje = Embalaje.IdEmbalaje AND Productos_1.IdEmpresa = Embalaje.IdEmpresa INNER JOIN
                         Tercero ON Productos_1.IdCliente = Tercero.IdTercero AND Productos_1.IdEmpresa = Tercero.IdEmpresa INNER JOIN
                         GrupoProductos ON Productos_1.IdGrupo = GrupoProductos.IdProductoGrupo AND Productos_1.IdEmpresa = GrupoProductos.IdEmpresa INNER JOIN
                         ProductosCodigos ON Productos_1.IdProducto = ProductosCodigos.IdProducto AND Tercero.IdTercero = ProductosCodigos.IdCliente AND Productos_1.IdEmpresa = ProductosCodigos.IdEmpresa
WHERE        (Productos_1.IdEmpresa = @IdEmpresa) AND (Productos_1.IdCliente = @IdCliente) AND (ProductosCodigos.Principal = 'true') AND (Productos_1.IdEstado IN (1, @IdEstado))</CommandText>
